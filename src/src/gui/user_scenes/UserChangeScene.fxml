<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.controllers.user_controllers.UsersChange">
   <children>
      <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab text="Select user">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="display" layoutY="12.0" prefHeight="284.0" prefWidth="600.0" promptText="click to show src.users" />
                        <Label layoutX="386.0" layoutY="321.0" prefHeight="25.0" prefWidth="99.0" text="Index of user:">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="userIndex" layoutX="489.0" layoutY="321.0" prefHeight="25.0" prefWidth="97.0" />
                        <Button fx:id="btnUserRefresh" layoutX="14.0" layoutY="321.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="104.0" text="Show" />
                        <Button layoutX="131.0" layoutY="321.0" mnemonicParsing="false" onAction="#actionHome" prefHeight="25.0" prefWidth="102.0" text="Go back" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Select company">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="displaySecond" layoutY="12.0" prefHeight="284.0" prefWidth="600.0" promptText="click to show companies" />
                        <Label layoutX="271.0" layoutY="321.0" prefHeight="25.0" prefWidth="214.0" text="Index of insurance company:">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="companyIndex" layoutX="489.0" layoutY="321.0" prefHeight="25.0" prefWidth="97.0" />
                        <Button fx:id="btnRefreshCompanies" layoutX="14.0" layoutY="321.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="105.0" text="Show" />
                        <Button layoutX="130.0" layoutY="321.0" mnemonicParsing="false" onAction="#actionHome" prefHeight="25.0" prefWidth="102.0" text="Go back" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Select program">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="displayThird" layoutY="12.0" prefHeight="284.0" prefWidth="600.0" promptText="click to show programs" />
                        <Label layoutX="271.0" layoutY="321.0" prefHeight="25.0" prefWidth="214.0" text="Index of insurance program:">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="programIndex" layoutX="489.0" layoutY="321.0" prefHeight="25.0" prefWidth="97.0" />
                        <Button fx:id="btnRefreshPrograms" layoutX="14.0" layoutY="321.0" mnemonicParsing="false" prefWidth="101.0" text="Show" />
                        <Button layoutX="130.0" layoutY="321.0" mnemonicParsing="false" onAction="#actionHome" prefHeight="25.0" prefWidth="102.0" text="Go back" />
                        <Button layoutX="433.0" layoutY="209.0" mnemonicParsing="false" onAction="#confirmChange" prefHeight="79.0" prefWidth="153.0" text="Confirm change" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
