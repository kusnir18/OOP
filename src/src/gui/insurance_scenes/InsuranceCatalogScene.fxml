<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.controllers.insurance_controllers.InsuranceCatalogShow">
    <children>
        <TabPane prefHeight="394.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Select company">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TextArea fx:id="display" layoutY="14.0" prefHeight="284.0" prefWidth="600.0" promptText="click to show companies" />
                                <Label layoutX="271.0" layoutY="321.0" prefHeight="25.0" prefWidth="214.0" text="Index of insurance company:">
                                    <font>
                                        <Font size="16.0" />
                                    </font></Label>
                                <TextField fx:id="txtIndex" layoutX="489.0" layoutY="321.0" prefHeight="25.0" prefWidth="97.0" />
                        <Button layoutX="142.0" layoutY="321.0" mnemonicParsing="false" onAction="#actionHome" prefHeight="25.0" prefWidth="102.0" text="Go back" />
                        <Button layoutX="14.0" layoutY="321.0" mnemonicParsing="false" onAction="#actionRefresh" text="Show companies" />
                            </children></AnchorPane>
                    </content>
                </Tab>
                <Tab text="Catalog">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="365.0" prefWidth="496.0">
                            <children>
                                <TextArea fx:id="displaySecond" layoutY="14.0" prefHeight="284.0" prefWidth="600.0" />
                                <Button layoutX="497.0" layoutY="326.0" mnemonicParsing="false" onAction="#actionHome" prefHeight="25.0" prefWidth="90.0" text="Go Back" />
                                <Button layoutX="396.0" layoutY="326.0" mnemonicParsing="false" onAction="#actionShow" prefHeight="25.0" prefWidth="90.0" text="Show" />
                            </children></AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
